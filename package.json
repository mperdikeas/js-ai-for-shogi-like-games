{
  "name": "ai-for-shogi-like-games",
  "version": "0.0.12",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -fr lib/",
    "prebuild": "mkdir -p lib && for f in $(find src/ -iname *.js | cut -c5-) ; do cp src/$f lib/$f.flow; done",
    "build": "babel src --out-dir lib --source-maps",
    "build-watch": "babel src --out-dir lib --source-maps --watch",
    "start": "npm run build && node lib/index.js",
    "flow-check": "flow check",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "npm run build && mocha --require source-map-support/register --compilers js:babel-register",
    "test-watch": "npm run build && mocha --require source-map-support/register --compilers js:babel-register --watch",
    "preglobalInstallOnThisMachine": "npm run clean && npm run flow-check && npm run test",
    "globalInstallOnThisMachine": "npm install -g .",
    "prepublish": "(in-publish && npm run clean && flow check && npm run test && npm run build) || not-in-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mperdikeas/js-ai-for-shogi-like-games.git"
  },
  "bugs": {
    "url": "https://github.com/mperdikeas/js-ai-for-shogi-like-games/issues"
  },
  "homepage": "https://github.com/mperdikeas/js-ai-for-shogi-like-games#readme",
  "keywords": [
    "shogi",
    "AI",
    "boardgames"
  ],
  "author": "Marcus Junius Brutus",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "flow-bin": "^0.27.0",
    "in-publish": "^2.0.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-register": "^6.14.0",
    "geometry-2d": "*",
    "lodash": "^4.10.0",
    "source-map-support": "^0.4.0"
  }
}
